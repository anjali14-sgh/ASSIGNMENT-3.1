void plusMinus(vector<int> arr) {
    int pos = 0;
    int zero = 0;
    int neg = 0;
    
     int n = arr.size();
     
     for (int i=0; i<n; i++)
     {
         if (arr[i] > 0)
          pos ++;
          
          else if (arr[i]< 0)
          neg ++;
          
          else 
          zero ++;
     }
      
